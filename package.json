  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --runInBand --silent",
    "test-with-coverage": "react-scripts test --coverage --runInBand --silent",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "react-hooks/exhaustive-deps": "off"
        }
      }
    ]
  },
  "jest": {
    "restoreMocks": true,
    "clearMocks": true,
    "resetMocks": true,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts(x)?",
      "!<rootDir>/src/utils/apis/index.ts"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@affinidi/eslint-config": "^2.0.0-beta.0",
    "@affinidi/prettier-config": "^2.0.0-beta.0",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.1.7",
    "history": "^5.0.0",
    "node-sass": "^4.14.1"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "jsoneditor": "^7.0.0 || ^8.0.0 || ^9.0.0"
  }
